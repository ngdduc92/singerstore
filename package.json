{
  "name": "singerstore",
  "version": "1.0.0",
  "description": "singerstore",
  "main": "src/shopservice/server/index.js",
  "scripts": {
    "setup": "node -r esm src/shopservice/server/setup.js",
    "compile": "babel src/shopfront --out-dir dist/shopfront --presets=@babel/preset-react --plugins=@babel/plugin-proposal-class-properties --quiet",
    "compile:watch": "babel src/shopfront --out-dir dist/shopfront --presets=@babel/preset-react --plugins=@babel/plugin-proposal-class-properties --watch",
    "webpack:admin": "webpack --config webpack.config.shopadmin.js --mode production",
    "webpack:client": "webpack --config webpack.config.shopfront.js --mode production",
    "webpack:admin:watch": "webpack --config webpack.config.shopadmin.js --mode development --watch",
    "webpack:client:watch": "webpack --config webpack.config.shopfront.js --mode development --watch",
    "theme:build": "babel theme/src --out-dir theme/dist --presets=@babel/preset-react --plugins=@babel/plugin-proposal-class-properties --quiet",
    "theme:build:watch": "babel theme/src --out-dir theme/dist --presets=@babel/preset-react --plugins=@babel/plugin-proposal-class-properties --watch",
    "build": "npm run compile && npm run theme:build && npm run webpack:client && npm run webpack:admin",
    "build:watch": "concurrently \"npm run compile:watch\" \"npm run theme:build:watch\" \"npm run webpack:client:watch\" \"npm run webpack:admin:watch\"",
    "start:shop-api": "node -r esm src/shopservice/server/index.js",
    "start:shop-ui": "node -r esm dist/shopfront/server/index.js",
    "start": "concurrently npm:start:*",
    "debug:shop-api": "nodemon --inspect=localhost:9001 --watch src/shopservice/ --watch config/server.js -r esm src/shopservice/server/index.js",
    "debug:shop-ui": "nodemon --inspect=localhost:9000 --watch dist/ --watch theme/ --watch public/ -r esm dist/shopfront/server/index.js",
    "debug": "concurrently \"npm run build:watch\" \"npm:debug:*\"",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "prettier": "prettier --write \"src/**/*.js\" \"theme/**/*.js\"",
    "precommit": "lint-staged",
    "apidoc": "gulp apidoc"
  },
  "lint-staged": {
    "*.js": [
      "prettier --write",
      "git add"
    ]
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ngdduc92/singerstore.git"
  },
  "keywords": [
    "ecommerce",
    "shopping cart",
    "shop",
    "cms"
  ],
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/ngdduc92/singerstore/issues"
  },
  "homepage": "https://github.com/ngdduc92/singerstore#readme",
  "devDependencies": {
    "babel-eslint": "^10.0.1",
    "clean-webpack-plugin": "^0.1.19",
    "concurrently": "^4.1.0",
    "eslint": "^5.16.0",
    "eslint-config-airbnb": "^17.1.0",
    "eslint-config-prettier": "^4.1.0",
    "eslint-plugin-import": "^2.16.0",
    "eslint-plugin-jsx-a11y": "^6.2.1",
    "eslint-plugin-prettier": "^3.0.1",
    "eslint-plugin-react": "^7.12.4",
    "esm": "3.0.80",
    "husky": "^1.3.1",
    "lint-staged": "^7.3.0",
    "nodemon": "^1.19.0",
    "webpack": "^4.29.6",
    "webpack-cli": "^3.3.0"
  },
  "dependencies": {
    "@babel/cli": "^7.2.3",
    "@babel/core": "^7.4.0",
    "@babel/plugin-proposal-class-properties": "^7.4.0",
    "@babel/plugin-syntax-dynamic-import": "^7.2.0",
    "@babel/polyfill": "^7.4.0",
    "@babel/preset-env": "^7.4.1",
    "@babel/preset-react": "^7.0.0",
    "@material-ui/core": "^3.9.3",
    "@material-ui/icons": "^3.0.2",
    "atob": "^2.1.2",
    "babel-loader": "^8.0.5",
    "bcrypt": "^3.0.6",
    "body-parser": "^1.18.3",
    "chart.js": "^2.8.0",
    "cookie-parser": "^1.4.4",
    "cross-fetch": "^2.2.2",
    "crypto": "^1.0.1",
    "css-loader": "^2.1.1",
    "cssnano": "^4.1.0",
    "singerstore-api-client": "file:../singerstore-api-client",
    "express": "^4.16.4",
    "express-jwt": "^5.3.1",
    "formidable": "^1.2.1",
    "fs-extra": "^7.0.1",
    "gulp": "^4.0.2",
    "gulp-apidoc": "^0.2.8",
    "handlebars": "^4.1.1",
    "helmet": "^3.16.0",
    "html-webpack-plugin": "^3.2.0",
