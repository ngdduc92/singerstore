    "redux-form": "^7.4.2",
    "redux-form-material-ui": "^4.3.4",
    "redux-thunk": "^2.3.0",
    "response-time": "^2.3.2",
    "sass-loader": "^7.1.0",
    "sitemap": "^2.1.0",
    "slug": "^1.0.0",
    "stripe": "^6.28.0",
    "theme": "file:theme",
